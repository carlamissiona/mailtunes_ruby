<%= form_with url: search_radios_path, data: { controller: 'debounce', debounce_target: 'form' }, class: "form-inline subscription-form",id: "searchform", method: :get , format: :turbo_stream do |form| %>
  <%= form.search_field :name_search,class: "form-control" , data: { action: "input->debounce#search" } ,value: params[:name_search], oninput: "this.form.requestSubmit()" %> 
<% end %>
  
<div id="search_results"></div>
 
 